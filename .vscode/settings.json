  "aiXcoder.showTrayIcon": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": ["source.fixAll.format", "source.fixAll.eslint"],
  "editor.tabSize": 2,
  "workbench.colorCustomizations": {
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#1f6fd0",
